{
  "name": "i-watched-a-movie",
  "private": true,
  "description": "Personal movie catalogue.",
  "version": "1.0.0",
  "author": "Ivo Angelov <jc@jagdcake.com>",
  "dependencies": {
    "@types/react-helmet": "^5.0.15",
