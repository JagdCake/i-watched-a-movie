    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-plugin-typescript": "^2.10.0",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-source-pg": "^0.1.4",
    "gatsby-transformer-sharp": "^2.10.0",
    "postcss": "^8.2.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^10.4.9",
